+    @Test
+    void testGetRouteById_Success() {
+        when(routeRepository.findById(fixedRouteId)).thenReturn(Mono.just(route1));
+        when(routeQueryService.getRouteById(fixedRouteId)).thenReturn(Mono.just(route1));
